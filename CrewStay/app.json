{
  "expo": {
    "name": "CrewStay",
    "slug": "crewstay",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "scheme": "crewstay",
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": false,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}
